generator client {
  provider = "prisma-client-js"
 engineType = "binary"
}

datasource db {
  provider = "postgresql"
  url      = env("DATABASE_URL")
}

model MuscleGroup {
  id          Int         @id @default(autoincrement())
  muscle      String      @unique
  description String      @default("N/A")
  MV_MIN      Int
  MV_MAX      Int
  MEV_MIN     Int
  MEV_MAX     Int
  MAV_MIN     Int
  MAV_MAX     Int
  MRV_MIN     Int
  MRV_MAX     Int
  frequency   String?
  article_url String?
  excercises  Excercise[]
  images      Image[]
}

model Excercise {
  id            Int          @id @default(autoincrement())
  muscleGroupId Int?
  name          String
  url           String
  axis          String?
  MuscleGroup   MuscleGroup? @relation(fields: [muscleGroupId], references: [id])
}

model Image {
  id            Int          @id @default(autoincrement())
  muscleGroupId Int?
  path          String
  MuscleGroup   MuscleGroup? @relation(fields: [muscleGroupId], references: [id])
}
